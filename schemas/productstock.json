{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "JSON Schema for a OneSaas Product Stock",
    "type": "object",
    "properties": {
        "RemoteUid": {
            "description": "Unique identifier of this entity in the remote system.",
            "type": "string"
        },
        "BuyingPrice": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "SellingPrice": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "StockAllocated": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "StockAvailable": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "StockOnHand": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        }
    }
}